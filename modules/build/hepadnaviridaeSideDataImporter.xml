        <identifier>true</identifier>
        <header>sequenceID</header>
        <property>sequenceID</property>
    </textFileColumn>

    <textFileColumn>
        <header>virus_name</header>
        <property>name</property>
    </textFileColumn>
    
    <textFileColumn>
        <header>virus_full_name</header>
        <property>full_name</property>
    </textFileColumn>
 
    <textFileColumn>
        <header>virus_genus</header>
        <property>genus</property>
    </textFileColumn>
    
    <textFileColumn>
        <header>clade</header>
        <property>clade</property>
    </textFileColumn>


    <textFileColumn>
        <header>core_shift</header>
        <property>core_shift</property>
    </textFileColumn>
 
    <textFileColumn>
        <header>variable_region_type</header>
        <property>variable_region_type</property>
    </textFileColumn>

 
     <textFileColumn>
        <header>host_sci_name</header>
        <property>isolate_data.host_sci_name</property>
    </textFileColumn>

    <skipMissing>true</skipMissing>

</textFilePopulator>
