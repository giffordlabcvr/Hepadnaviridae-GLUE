<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<textFilePopulator> 

    <columnDelimiterRegex>\t</columnDelimiterRegex>
    
    <textFileColumn>
        <identifier>true</identifier>
        <header>id</header>
        <property>sequenceID</property>
    </textFileColumn>

    <textFileColumn>
        <header>locus_id</header>
        <property>name</property>
    </textFileColumn>
    
    <textFileColumn>
        <header>full_name</header>
        <property>full_name</property>
    </textFileColumn>
  
    <textFileColumn>
        <header>family</header>
        <property>family</property>
    </textFileColumn>
    
    <textFileColumn>
        <header>genus</header>
        <property>genus</property>
    </textFileColumn>
    
    <textFileColumn>
        <header>host_sci_name</header>
        <property>locus_data.host_sci_name</property>
    </textFileColumn>


    <skipMissing>true</skipMissing>

</textFilePopulator>
